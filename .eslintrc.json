{
    "extends": ["react-app"],
    "rules": {
        "curly": "warn",
        "@typescript-eslint/no-unused-vars": [
            1,
            { "vars": "all", "varsIgnorePattern": "^_", "args": "after-used", "argsIgnorePattern": "^_" }
        ]
    },
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}
