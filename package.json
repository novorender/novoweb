        "@mui/material": "^5.15.10",
        "@mui/x-date-pickers": "^7.10.0",
        "@novorender/api": "0.5.4",
        "@novorender/data-js-api": "0.2.26",
        "@novorender/webgl-api": "0.4.59",
        "@reduxjs/toolkit": "^2.0.1",
        "@use-gesture/react": "^10.2.14",
        "@visx/tooltip": "^2.10.0",
        "@visx/visx": "^3.8.0",
        "d3-array": "^3.2.0",
        "date-fns": "^2.27.0",
        "detect-gpu": "^5.0.33",
        "express": "^4.19.2",
        "express-static-gzip": "^2.1.7",
